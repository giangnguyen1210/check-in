<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.checkin.mapper.BranchMapper">
    <insert id="createBranch">
        insert into branch(name) values (#{name})
    </insert>
    <select id="listBranch" resultType="com.checkin.dto.response.BranchResponse">
        select * from branch
    </select>
</mapper>