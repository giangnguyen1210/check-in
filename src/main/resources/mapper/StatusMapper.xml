<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.checkin.mapper.StatusMapper">

    <insert id="createStatus">
        insert into status(name) values (#{name})
    </insert>
    <select id="listStatus" resultType="com.checkin.dto.response.StatusResponse">
        select * from status
    </select>
    <select id="checkStatusExist" resultType="java.lang.Integer">
        select * from status where name=#{name}
    </select>
</mapper>